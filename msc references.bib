@article{prajapati2019bequeath,
  title = {Bequeath Life to Death: {{State-of-art}} Review on Reverse Logistics},
  shorttitle = {Bequeath Life to Death},
  author = {Prajapati, Himanshu and Kant, Ravi and Shankar, Ravi},
  year = {2019},
  month = feb,
  journal = {Journal of Cleaner Production},
  volume = {211},
  pages = {503--520},
  issn = {0959-6526},
  doi = {10.1016/j.jclepro.2018.11.187},
  urldate = {2022-10-31},
  abstract = {Reverse logistics is gaining attention of the industrialist and academicians due to enormous waste generation leading to increased environmental unease. The objective of this study is to perform a systematic review of the available reverse logistics literature, highlighting the research gap and setting future research directions. Content Analysis methodology is used in this literature review with Abductive research approach. 449 relevant articles are selected containing the word ``reverse logistics'' in their title, abstract and keywords, which were published in a well-known database. These articles are meticulously evaluated and categorized in 11 different categories according to their structural dimensions and contents. This work identifies: (i) various research design and methodology used in reverse logistics literature, (ii) most explored areas of reverse logistics, and discusses the sector where the exploration can be redirected, (iii) most explored industries, (iv) most used algorithms, Operation Research (OR)/ Mathematical tool, data analysis technique and Multi Criteria Decision Making (MCDM) methods, and (v) enablers and barriers to reverse logistics. This study will help researchers to get opportunities in identifying the new dimensions of research in the field of reverse logistics.},
  langid = {english},
  keywords = {Abductive research,cite-lb,Content analysis,economic-lb,Literature review,Reverse logistics,RL},
  note = {good review structure
\par
has journals with h index that reverse logistics papers are usually published in
\par
different analysis methods (good for supporting the method I choose to analyze my data). profit maximization, cost minimization
\par
really good summary of enablers as well as barriers (table 17 and 18)
\par
\textbf{\textbullet{} }The idea of reverse logistics started back in 1982 but the research gain momentum after 2008 as can be seen from the analysis of~\href{https://www.sciencedirect.com/science/article/pii/S095965261833590X#fig4}{Fig.~4}. The globalization of the economy has created an agitation in organizations forcing to adopt changes in operations and in client assistance (\href{https://www.sciencedirect.com/science/article/pii/S095965261833590X#bib73}{Vaz et~al., 2013}). The increase in number of publication is about 300\% since 2008. Substantial amount of publishers (62 numbers) and journals (174 numbers) are involved in publishing articles on reverse logistics (activities).
\par
\textbf{\textbullet{} }Before 2005, there were very limited articles in number. Later reverse logistics was given due attention but the research was mainly concentrated in developed countries (United States of America, Spain, Singapore etc.). A significant rise in research from other countries (such as India, China and Iran) was shown after 2013 onwards (\href{https://www.sciencedirect.com/science/article/pii/S095965261833590X#tbl7}{Table~7}). Industrial growth in developing countries has risen sharply in the twentieth century. Also, shorter life cycle and technological improvement of products is responsible for enormous waste generation thereby developing a need to study reverse logistics.
\par
Quantitative research methodologies such as~\href{https://www.sciencedirect.com/topics/engineering/mathematical-modelling}{mathematical modeling}, simulation and survey methods are the dominant methods in research constituting more than 55\% of the research. Mathematical modeling is the most used research method in the literature. A mathematical model helps industries to optimize their total cost in order to enhance profit (\href{https://www.sciencedirect.com/science/article/pii/S095965261833590X#bib5}{Bazen et~al., 2016}). Amongst them many models are focused on network design, facility location and routing. Most of the mathematical and simulation model are verified by numerical examples. A realistic view can be gained if verification to be made through case study approach (Only 17\% of articles on modeling did it) (\href{https://www.sciencedirect.com/science/article/pii/S095965261833590X#tbl9}{Table~9}). Also, many models assume that their variables as deterministic for the analysis, but in real they are not. Very few researches are available with~\href{https://www.sciencedirect.com/topics/engineering/stochastic-behavior}{stochastic behavior}~of variables (\href{https://www.sciencedirect.com/science/article/pii/S095965261833590X#bib56}{Pokharel and Mutha, 2009}).},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\WM2N5IRV\\Prajapati et al. - 2019 - Bequeath life to death State-of-art review on rev.pdf;C\:\\Users\\Mabel Karani\\Zotero\\storage\\L59KCVHW\\S095965261833590X.html}
}

