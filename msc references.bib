@article{kenter2011importance,
  title = {The Importance of Deliberation in Valuing Ecosystem Services in Developing Countries - {{Evidence}} from the {{Solomon Islands}}},
  author = {Kenter, Jasper O. and Hyde, Tony and Christie, Michael and Fazey, Ioan},
  year = {2011},
