@article{mitchell2008altered,
  title = {Altered Landscapes, Altered Livelihoods: {{The}} Shifting Experience of Informal Waste Collecting during {{Hanoi}}'s Urban Transition},
  author = {Mitchell, Carrie L.},
  year = {2008},
  month = nov,
  journal = {Placing Splintering Urbanism},
