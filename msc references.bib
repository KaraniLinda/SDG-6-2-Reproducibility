@article{mpanangombe2018biowaste,
  title = {A {{Biowaste Treatment Technology Assessment}} in {{Malawi}}},
  author = {Mpanang'ombe, Wrixon and Tilley, Elizabeth and Zabaleta, Imanol and Zurbr{\"u}gg, Christian},
  year = {2018},
