@article{mishra2018value,
  title = {Value Creation from Circular Economy-Led Closed Loop Supply Chains: A Case Study of Fast-Moving Consumer Goods},
  shorttitle = {Value Creation from Circular Economy-Led Closed Loop Supply Chains},
  author = {Mishra, Jyoti L. and Hopkinson, Peter G. and Tidridge, Gin},
  year = {2018},
