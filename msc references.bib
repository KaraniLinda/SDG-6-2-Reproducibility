@article{velis2021mismanagement,
  title = {Mismanagement of {{Plastic Waste}} through {{Open Burning}} with {{Emphasis}} on the {{Global South}}: {{A Systematic Review}} of {{Risks}} to {{Occupational}} and {{Public Health}}},
  shorttitle = {Mismanagement of {{Plastic Waste}} through {{Open Burning}} with {{Emphasis}} on the {{Global South}}},
  author = {Velis, Costas A. and Cook, Ed},
  year = {2021},
  month = jun,
