  author = {Jambeck, Jenna and Hardesty, Britta Denise and Brooks, Amy L. and Friend, Tessa and Teleki, Kristian and Fabres, Joan and Beaudoin, Yannick and Bamba, Abou and Francis, Julius and Ribbink, Anthony J. and Baleta, Tatjana and Bouwman, Hindrik and Knox, Jonathan and Wilcox, Chris},
  year = {2018},
