@article{fogtjacobsen2022drivers,
  title = {Drivers of and Barriers to Consumers' Plastic Packaging Waste Avoidance and Recycling \textendash{} {{A}} Systematic Literature Review},
  author = {Fogt Jacobsen, Lina and Pedersen, Susanne and Th{\o}gersen, John},
