@article{schoebitz2017gis,
  title = {{{GIS Analysis}} and {{Optimisation}} of {{Faecal Sludge Logistics}} at {{City-Wide Scale}} in {{Kampala}}, {{Uganda}}},
  author = {Schoebitz, Lars and Bischoff, Fabian and Lohri, Christian Riuji and Niwagaba, Charles B. and Siber, Rosi and Strande, Linda},
  year = {2017},
