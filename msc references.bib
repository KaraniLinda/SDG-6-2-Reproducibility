  shorttitle = {The {{Kansas City Transportation}} and {{Local-Scale Air Quality Study}} ({{KC-TRAQS}})},
  author = {Kimbrough, Sue and Krabbe, Stephen and Baldauf, Richard and Barzyk, Timothy and Brown, Matthew and Brown, Steven and Croghan, Carry and Davis, Michael and Deshmukh, Parikshit and Duvall, Rachelle and Feinberg, Stephen and Isakov, Vlad and Logan, Russell and McArthur, Tim and Shields, Amy},
