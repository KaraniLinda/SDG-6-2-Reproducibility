  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\AGLZIJE7\\Ramadan et al. - 2022 - Activity and emission inventory of open waste burn.pdf}
}

@article{ramadan2022comprehensive,
  title = {A Comprehensive Review of Domestic-Open Waste Burning: Recent Trends, Methodology Comparison, and Factors Assessment},
  shorttitle = {A Comprehensive Review of Domestic-Open Waste Burning},
