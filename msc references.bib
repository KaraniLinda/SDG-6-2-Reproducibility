@article{rupf2015barriers,
  title = {Barriers and Opportunities of Biogas Dissemination in {{Sub-Saharan Africa}} and Lessons Learned from {{Rwanda}}, {{Tanzania}}, {{China}}, {{India}}, and {{Nepal}}},
  author = {Rupf, Gloria V. and Bahri, Parisa A. and {de Boer}, Karne and McHenry, Mark P.},
  year = {2015},
