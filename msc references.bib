@article{alessi2020mechanical,
  title = {Mechanical Separation of Impurities in Biowaste: {{Comparison}} of Four Different Pretreatment Systems},
  shorttitle = {Mechanical Separation of Impurities in Biowaste},
  author = {Alessi, Alessia and Lopes, Alice do Carmo Precci and M{\"u}ller, Wolfgang and Gerke, Fr{\'e}d{\'e}ric and Robra, Sabine and Bockreis, Anke},
  year = {2020},
