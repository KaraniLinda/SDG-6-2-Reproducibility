@article{dhahak2019slow,
  title = {Slow Pyrolysis of Polyethylene Terephthalate: {{Online}} Monitoring of Gas Production and Quantitative Analysis of Waxy Products},
  shorttitle = {Slow Pyrolysis of Polyethylene Terephthalate},
  author = {Dhahak, Asma and Hild, Guillaume and Rouaud, Mathieu and Mauviel, Guillain and {Burkle-Vitzthum}, Val{\'e}rie},
  year = {2019},
  month = sep,
  journal = {Journal of Analytical and Applied Pyrolysis},
  volume = {142},
  pages = {104664},
  issn = {0165-2370},
  doi = {10.1016/j.jaap.2019.104664},
  urldate = {2022-04-26},
  abstract = {The slow pyrolysis of polyethylene terephthalate (PET) was investigated over a range of final temperatures from 410\,\textdegree C to 480\,\textdegree C yielding a wide variety of species. The objective of the current study was to provide thermal dependent evolution profile for mass loss and pyrolysis gas, in addition to the partial quantification of the waxy products. The polymer degradation was first characterized by thermogravimetric analysis. Many parameters were studied such as final temperature, heating rate, flow rate and the presence or not of a lid on the crucible. The slow pyrolysis of PET was then performed in a horizontal tubular reactor at 5\,\textdegree C/min. The pyrolysis products were analysed, focusing especially on gaseous and waxy fractions. The gas emitted was monitored online by {$\mu$}-GC which allowed establishing their real time evolution profile. Carbonyl species were analyzed using HPLC. As a result, the gas contained mainly acetaldehyde, CO and CO2, and to a smaller extent ethylene and benzene. Benzene profile exhibited two peaks, indicating that there may be two steps or two main reactions in the release of this compound. The waxy products were characterized by GC/MS-FID and FTIR techniques. FTIR showed that the main chemical moieties of the waxy products were carboxylic acids and esters. GC/MS-FID confirmed that these pyrolysates contained mainly benzoic acid and its derivatives and monovinyl terephthalate. The quantitative analysis showed that the highest production of benzoic acid was obtained at 430\,\textdegree C. Its yield is 11\% of initial PET mass.},
  langid = {english},
  keywords = {Gas monitoring,Online analysis,Polyethylene terephthalate,Pyrolysis,Quantification,Waxy characterization},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\RSEKAFP2\\S0165237019303249.html}
}

