@book{mahdjoub2020mining,
  title = {'{{MINING}}' {{THE VILLAGE}}: {{BUILDING WITH WASTE GLASS IN CHEMBE}}, {{MALAWI}}},
  shorttitle = {'{{MINING}}' {{THE VILLAGE}}},
  author = {Mahdjoub, Noredine and Tilley, Elizabeth and Kalina, Marc and Augustine, Alex and Trois, Cristina},
  year = {2020},
  month = nov,
  abstract = {Landfill mining, a process of extracting and processing waste from stored deposits, has been touted within waste management discourses as circular economy strategy for sustainable resource recovery and reuse. However, within the Global South, where many contexts lack engineered landfilles, or even consolidated dumpsites, what relevance does the concept hold? The purpose of this paper is to investigate the feasibility of a 'mining' strategy for recovering a reusing stockpiled waste glass in Chembe, Malawi within burnt clay bricks and hardened concrete, the two most common building materials within the village. Findings suggest that a proposed 'village mining' strategy, that mines stored stocks of waste glass throughout the village for use in the local consturctoin industry, could benefit multiple stakeholders, with crushed waste glass content of up to 10\% demonstrating performancing enhancing qualities within burnt clay bricks in certain building applications. Developing this process could lead to income generation for the Chembe village brick makers and embracing this new concept could trigger consistent and intensive waste glass mining at local village dumpsites.},
  keywords = {Malawi,Not cited}
}

@article{mahdjoub2021innovating,
  title = {Innovating Traditional Building Materials in {{Chembe}}, {{Malawi}}: Assessing Post-Consumer Waste Glass and Burnt Clay Bricks for Performance and Circularity},
  author = {Mahdjoub, Noredine and Kalina, Marc and Augustine, Alex and Tilley, Elizabeth},
  year = {2021},
  journal = {International Journal of Sustainable Engineering},
  volume = {14},
  number = {4},
  pages = {874--883},
  publisher = {{Taylor \& Francis}},
  address = {{London}},
  issn = {1939-7046},
  doi = {10.1080/19397038.2021.1889706},
  abstract = {Across the Global South, post-consumer waste glass is an often dumped, and under-utilised resource. Even in Malawi, with widespread return schemes, many barriers exist, inhibiting reuse, and necessitating appropriate solutions. The purpose of this article is to evaluate the performance of post-consumer waste glass as a coarse aggregate within burnt clay bricks, and to assess the feasibility for the recovery of this waste material from dumped stocks within Chembe. Using a brick design and testing methodology, which could be replicated within a rural African context, we tested a range of glass additions (both quantity and size of particle) for compressive strength and water absorption properties. Our results suggest that waste glass can function as a performance enhancer, with positive effects on compressive strength observed at up to 10\% crushed waste glass content. These findings support existing literature on glass waste additions, yet show that optimal results can also be had with post-consumer waste glass and in low income, and less technology-reliant contexts. Moreover, our findings suggest that current above-ground stocks of waste glass are sufficient to support the production of hybrid building materials for decades, however further innovation is necessary in order to achieve a sustainable mode of practice.},
  copyright = {Creative Commons Attribution-NonCommercial-NoDerivatives 4.0 International},
  langid = {english},
  keywords = {Africa,Alternative building materials,Circular economy,Landfill mining,Solid waste},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\KD4T23N7\\Mahdjoub et al_2021_Innovating traditional building materials in Chembe, Malawi.pdf}
}

