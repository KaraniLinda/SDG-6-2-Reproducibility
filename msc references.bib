@article{jang2020recycling,
  title = {Recycling and Management Practices of Plastic Packaging Waste towards a Circular Economy in {{South Korea}}},
  author = {Jang, Yong-Chul and Lee, Gain and Kwon, Yuree and Lim, Jin-hong and Jeong, Ji-hyun},
