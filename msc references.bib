@incollection{cordell2016transforming,
  title = {Transforming Cities: Securing Food and Clean Waterways through a Transdisciplinary Phosphorus Approach},
  booktitle = {Transdisciplinary {{Research}} and {{Practice}} for {{Sustainability Outcomes}}},
  author = {Cordell, Dana and Metson, Genevi{\'e}ve S. and Iwaniec, David M. and Bui, Thuy T. and Childers, Daniel L. and Dao, Nguyet and Dang, Huyen T. T. and Davidson, J. and Jacobs, Brent and Kumwenda, Save and Morse, Tracy and Nguyen, Viet-Anh and Thole, Bernard and Tilley, Elizabeth A.},
  editor = {Fam, Dena and Palmer, Jane and Riedy, Chris and Mitchell, Cynthia},
  year = {2016},
  month = nov,
  edition = {First},
  pages = {163--178},
  publisher = {{Routledge}},
  address = {{London, UK}},
  doi = {10.4324/9781315652184-20},
  urldate = {2022-12-13},
  abstract = {As an essential input to crop growth via soil reserves or fertilizer, phosphorus underpins global food security. Without phosphorus, food could not be produced, yet phosphorus is mined from fi nite reserves, most of which are controlled by only a few countries1 (UNEP 2011; Jasinski 2015; Cordell and White 2014). Fertilizer prices are likely to increase as fi nite reserves become critically scarce. Globally, a billion farmers and their families cannot access fertilizer markets and many rely on phosphorus-defi cient soils that produce low crop yields (IFPRI 2003). Moreover, mismanagement along the phosphorus supply chain from mine to fi eld to fork has resulted in massive losses and waste, which largely ends up in waterways, causing nutrient pollution and algal blooms (Bennett, Carpenter and Caraco 2001). The global phosphorus challenge is inherently complex; it is as much about international relations as farm soil fertility. It transcends disciplines, sectors, and scales \textendash{} from geopolitics to ecology to nutrition. In this chapter, we describe and refl ect upon a new project using a novel transdisciplinary approach to address this phosphorus challenge.},
  isbn = {978-1-315-65218-4},
  langid = {english},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\2PCJIV5F\\Cordell et al_2016_Transforming cities.pdf}
}

