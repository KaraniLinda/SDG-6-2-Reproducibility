@incollection{roopnarain2020biogas,
  title = {Biogas {{Technology}} in {{Africa}}: {{An Assessment}} of {{Feedstock}}, {{Barriers}}, {{Socio-Economic Impact}} and the {{Way Forward}}},
  booktitle = {Biogas {{Production}}: {{From Anaerobic Digestion}} to a {{Sustainable Bioenergy Industry}}},
  author = {Roopnarain, Ashira and Ndaba, Busiswa and {Bello-Akinosho}, Maryam and {Bamuza-Pemu}, Emomotimi and Mukhuba, Mashudu and Nkuna, Rosina and Adeleke, Rasheed},
  editor = {Balagurusamy, Nagamani and Chandel, Anuj Kumar},
  year = {2020},
  pages = {415--445},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-030-58827-4_18},
  abstract = {In spite of the abundance and diversity of renewable energy resources in Africa, a dire energy crisis currently prevails. The adoption of biogas technology in Africa is promising due to its multitude of benefits including energy generation in the form of biogas, waste management and soil ameliorant production. Furthermore, biogas technology is particularly attractive in Africa due to the economic, environmental and social benefits that are associated with the technology as well as the favourable environmental conditions on the African continent that is conducive to the anaerobic digestion process. Anaerobic digestion of the array of organic waste streams that are available in Africa, which range from agricultural, municipal and industrial waste, could aid in the provision of a renewable energy source (biogas), thereby helping to mitigate energy poverty. The socio-economic benefits of adoption of biogas technology are vast. Technology adoption could result in improved health and agriculture, economic prosperity and social benefits. However, even though the technology is widely exploited in selected developing countries, adoption remains in its infancy on the African continent. This has been attributed to numerous factors that range from elevated initial capital costs, absence of support structures, lack of awareness as well as negative perceptions of the technology cast by past failures. Such hurdles need to be overcome to improve technology adoption and reap the multitude of benefits associated with the technology. This chapter provides a brief overview of the current status of biogas technology adoption in Africa with reference to both household and commercial anaerobic digesters. In addition, the feedstock that may be utilised and its availability for the anaerobic digestion process are highlighted. The societal perception of biogas technology and socio-economic impact associated with technology adoption are also outlined. Lastly, barriers impeding biogas technology adoption in Africa and strategies to promote its adoption are discussed.},
  isbn = {978-3-030-58827-4}
}

@article{rosaldo2016revolution,
  title = {Revolution in the {{Garbage Dump}}: {{The Political}} and {{Economic Foundations}} of the {{Colombian Recycler Movement}}, 1986-2011},
  author = {Rosaldo, Manuel},
  year = {2016},
  month = aug,
  journal = {Social Problems},
  volume = {63},
  number = {3},
  pages = {351--372},
  issn = {0037-7791},
  doi = {10.1093/socpro/spw015},
  urldate = {2022-02-19},
  abstract = {Flouting 150 years of reports on their political impotence, millions of informal workers have recently begun mobilizing for labor rights. What provoked this unexpected development? This article analyzes the Colombian informal recycler movement\textemdash a ``least likely'' case for successful mobilization due to the recyclers' extreme marginality and the Colombian state's violent repression of labor movements. The article argues that the rise of neoliberalism and the consolidation of democracy created political opportunities that conventional perspectives on the informal economy would not lead us to expect. Three specific links connected these macro-level transformations to increases in the recyclers' collective organizing capacity. First, technical, financial, and symbolic backing from non-governmental organizations (NGOs) enabled recyclers to develop innovative organizing models. Second, new human rights provisions contained in the Constitution of 1991 created an opening to challenge state policy. Third, the privatization of waste management spurred recyclers to action by leaving them with two clear-cut possibilities: waste corporations might permanently displace them, or recyclers might collectively organize to protect and improve their livelihoods.Desobedeciendo a 150 a\~nos de informes sobre impotencia pol\'itica, millones de trabajadores informales han comenzado recientemente a movilizarse por sus derechos laborales. \textquestiondown Qu\'e provoc\'o este desarrollo inesperado? Este art\'iculo analiza el caso del movimiento reciclador informal en Colombia\textemdash un caso ``menos probable'' de tener \'exito debido a la marginalidad extrema de los recicladores y la represi\'on violenta del estado colombiano hacia los movimientos obreros. El art\'iculo sostiene que el auge del neoliberalismo y la consolidaci\'on de la democracia crean oportunidades pol\'iticas que las perspectivas convencionales sobre la econom\'ia informal no llevar\'ia a esperar. Tres enlaces espec\'ificos conectaron a estas transformaciones de macronivel al aumento de la capacidad de organizaci\'on colectiva de los recicladores. En primer lugar, el apoyo t\'ecnico, financiero y simb\'olico de las ONG permitieron que los recicladores desarrollaran modelos innovadores de organizaci\'on. En segundo lugar, las nuevas disposiciones de derechos humanos en la Constituci\'on de 1991 crearon una apertura para desafiar la pol\'itica del estado. En tercer lugar, la privatizaci\'on de la gesti\'on de residuos estimul\'o a los recicladores a la acci\'on por lo que les dej\'o dos posibilidades claras: las empresas de desecho pueden desplazarlos de forma permanente o los recicladores podr\'ian organizarse colectivamente para proteger y mejorar sus medios de vida.}
}

