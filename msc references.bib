@book{godfrey2019solid,
  title = {Solid {{Waste Management}} in {{Africa}}: {{Governance Failure}} or {{Development Opportunity}}?},
  shorttitle = {Solid {{Waste Management}} in {{Africa}}},
  author = {Godfrey, Linda and Ahmed, Mohamed Tawfic and Gebremedhin, Kidane Giday and Katima, Jamidu H. Y. and Oelofse, Suzan and Osibanjo, Oladele and Richter, Ulf Henning and Yonli, Ars{\`e}ne H.},
