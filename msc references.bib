@article{kraay2014poverty,
  title = {Do {{Poverty Traps Exist}}? {{Assessing}} the {{Evidence}}},
  shorttitle = {Do {{Poverty Traps Exist}}?},
  author = {Kraay, Aart and McKenzie, David},
  year = {2014},
