@article{kylili2017policy,
  title = {Policy Trends for the Sustainability Assessment of Construction Materials: {{A}} Review},
  shorttitle = {Policy Trends for the Sustainability Assessment of Construction Materials},
  author = {Kylili, Angeliki and Fokaides, Paris A.},
  year = {2017},
