@article{siltaloppi2021sustainable,
  title = {Toward a Sustainable Plastics Value Chain: {{Core}} Conundrums and Emerging Solution Mechanisms for a Systemic Transition},
  shorttitle = {Toward a Sustainable Plastics Value Chain},
  author = {Siltaloppi, Jaakko and J{\"a}hi, Markus},
  year = {2021},
