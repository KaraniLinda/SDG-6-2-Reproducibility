@article{deng2020source,
  title = {Source Apportionment of Black Carbon Aerosols from Light Absorption Observation and Source-Oriented Modeling: An Implication in a Coastal City in {{China}}},
  shorttitle = {Source Apportionment of Black Carbon Aerosols from Light Absorption Observation and Source-Oriented Modeling},
  author = {Deng, Junjun and Guo, Hao and Zhang, Hongliang and Zhu, Jialei and Wang, Xin and Fu, Pingqing},
  year = {2020},
  month = nov,
  journal = {Atmospheric Chemistry and Physics},
  volume = {20},
  number = {22},
  pages = {14419--14435},
  publisher = {{Copernicus GmbH}},
  issn = {1680-7316},
  doi = {10.5194/acp-20-14419-2020},
  urldate = {2022-08-23},
  abstract = {{$<$}p{$><$}strong class="journal-contentHeaderColor"{$>$}Abstract.{$<$}/strong{$>$} Black carbon (BC) is the most important light-absorbing aerosol in the atmosphere. However, sources of atmospheric BC aerosols are largely uncertain, making it difficult to assess its influence on radiative forcing and climate change. In this study, year-round light-absorption observations were conducted during 2014 using an aethalometer in Xiamen, a coastal city in Southeast China. Source apportionment of BC was performed and temporal variations in BC sources were characterized based on both light absorption measurements and a source-oriented air quality model. The annual average concentrations of BC from fossil fuel (BC\textsubscript{ff}) and biomass burning (BC\textsubscript{bb}) by the aethalometer method were 2932\&thinsp;{$\pm$}\&thinsp;1444\&thinsp;ng\&thinsp;m\textsuperscript{-3} and 1340\&thinsp;{$\pm$}\&thinsp;542\&thinsp;ng\&thinsp;m\textsuperscript{-3}, contributing 66.7\&thinsp;\% and 33.3\&thinsp;\% to total BC, respectively. A sensitivity analysis was performed with different absorption \AA ngstr\"om exponent (AAE) values of fossil fuel combustion (\emph{{$\alpha$}}\textsubscript{ff}) and biomass burning (\emph{{$\alpha$}}\textsubscript{bb}), suggesting that the aethalometer method was more sensitive to changes in \emph{{$\alpha$}}\textsubscript{bb} than \emph{{$\alpha$}}\textsubscript{ff}. BC\textsubscript{bb} contribution exhibited a clear diurnal cycle, with the highest level (37.9\&thinsp;\%) in the evening rush hour and a seasonal pattern with the maximum (39.9\&thinsp;\%) in winter. Conditional probability function (CPF) analysis revealed the large biomass-burning contributions were accompanied by east-northeasterly and northerly winds. Backward trajectory indicated that air masses from North and East\textendash Central China were associated with larger biomass-burning contributions. Potential source contribution function (PSCF) and concentration-weighted trajectory (CWT) suggested that North and East\textendash Central China and Southeast Asia were potential sources of both BC\textsubscript{ff} and BC\textsubscript{bb}. The source-oriented modeling results showed that transportation, residential and open biomass burning accounting for 45.3\&thinsp;\%, 30.1\&thinsp;\% and 17.6\&thinsp;\% were the major BC sources. Among the three fuel catalogs, liquid fossil fuel (46.5\&thinsp;\%) was the largest source, followed by biomass burning (32.6\&thinsp;\%) and coal combustion (20.9\&thinsp;\%). Source contributions of fossil fuel combustion and biomass burning identified by the source-oriented model were 67.4\&thinsp;\% and 32.6\&thinsp;\%, respectively, close to those obtained by the aethalometer method. The findings provide solid support for controlling fossil fuel sources to limit the impacts of BC on climate change and environmental degradation in the relatively clean region in China.{$<$}/p{$>$}},
  langid = {english},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\ARED4TAG\\Deng et al. - 2020 - Source apportionment of black carbon aerosols from.pdf;C\:\\Users\\Mabel Karani\\Zotero\\storage\\7AYAFR7K\\2020.html}
}

@incollection{denis2020more,
  title = {More {{Urban Constructions}} for {{Whom}}? {{Drivers}} of {{Urban Built-Up Expansion Across}} the {{World}} from 1990 to 2015},
  shorttitle = {More {{Urban Constructions}} for {{Whom}}?},
  booktitle = {Theories and {{Models}} of {{Urbanization}}: {{Geography}}, {{Economics}} and {{Computing Sciences}}},
  author = {Denis, Eric},
  editor = {Pumain, Denise},
  year = {2020},
  series = {Lecture {{Notes}} in {{Morphogenesis}}},
  pages = {235--258},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-030-36656-8_13},
  urldate = {2023-02-02},
  abstract = {Since the 1990s, the conversion of land for urban use has never been more intense. Occurring under all latitudes, transcending the economic disparities as well as the political regimes, urban built-up expansion appears very much articulated to the global financial turn, which occurred after the progressive destatization of the money creation, which accelerated and globalized after the debt crisis of the 1980s. Using the Global Human Settlement Layer dataset on the degree of urbanization, we can assess that, worldwide, the built-up surface expanded by 243,000~km2 between 1990 and 2015 and the average urban density has declined slightly from 2775 inhabitants per km2 to 2756. After describing the national level dataset used and our methodology, we expose the regional nuances in the global built-up spreading and their relations to population growth. Then in a third section, we correlate those national trends with a set of significant economic indicators. We articulate it also to the individualization and progress of housing condition. Population growth remains the dominant factor determining urban built-up spread, but countries differ. Built-up growth did not respond linearly to the final demand for housing and therefore to demographic pressure. Building is a material good whose value is also interdependent with economic growth. The universal relationship between size and density specific to scaling laws is verified once again in the case of cities. However, economic density or concentration of wealth tends to take precedence over population mass to explain urban built-up expansion.},
  isbn = {978-3-030-36656-8},
  langid = {english},
  keywords = {Cited},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\2K2J5A4C\\Denis - 2020 - More Urban Constructions for Whom Drivers of Urba.pdf}
}

