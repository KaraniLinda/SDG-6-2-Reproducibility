@article{sm2019mobile,
  title = {Mobile Monitoring of Air Pollution Using Low Cost Sensors to Visualize Spatio-Temporal Variation of Pollutants at Urban Hotspots},
  author = {Nagendra, S. M. Shiva and Reddy Yasa, Pavan and Mv, Narayana and Khadirnaikar, Seema and {Pooja Rani}},
  year = {2019},
  month = jan,
  journal = {Sustainable Cities and Society},
  volume = {44},
  pages = {520--535},
  issn = {22106707},
  doi = {10.1016/j.scs.2018.10.006},
  urldate = {2022-11-21},
  abstract = {This paper deals with the development of a smart personal air quality mon- itoring system (SPAMS) for real time air quality monitoring. The commer- cially available low-cost CO, NO2, O3, PM, temperature (T) and humidity (RH) sensors along with the microcontroller and GPRS were integrated in SPAMS. The developed SPAMS was calibrated in the laboratory and vali- dated through field measurements. Field measurements were performed by walking on footpaths and travelling in the bus during various times of a day and different days in a week at selected locations in Chennai city, India. Re- sults indicated that the PM2.5 concentration was highest in busy traffic site (mean = 22.7 {$\pm$} 8.45 {$\mu$}g/m3) and lowest in urban background (mean = 9.3 {$\pm$} 5.75 {$\mu$}g/m3). Whereas, the CO concentration was lowest at beach road (mean = 0.26 {$\pm$} 0.01 ppm). Not much variation was observed in the average concentrations of NO2 and O3. On the other hand, while travelling in buses, higher concentration of CO and NO2 were observed during the morning and evening periods due to the peaking traffic flow. On contrary O3 concentration was found to be highest during afternoon due to photochemical reactions.},
  langid = {english},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\GCF7JMS9\\Sm et al. - 2019 - Mobile monitoring of air pollution using low cost .pdf}
