@article{ravikiran2018longterm,
  title = {Long-Term Observations of Black Carbon Aerosol over a Rural Location in Southern Peninsular {{India}}: {{Role}} of Dynamics and Meteorology},
  shorttitle = {Long-Term Observations of Black Carbon Aerosol over a Rural Location in Southern Peninsular {{India}}},
  author = {Ravi Kiran, V. and Talukdar, S. and Venkat Ratnam, M. and Jayaraman, A.},
  year = {2018},
  month = sep,
  journal = {Atmospheric Environment},
  volume = {189},
  pages = {264--274},
  issn = {13522310},
  doi = {10.1016/j.atmosenv.2018.06.020},
  urldate = {2022-08-23},
  abstract = {Ten years (2008\textendash 2017) of Black Carbon (BC) observations obtained using Aethalometer (AE-31) are analyzed to investigate the seasonal trends and temporal variabilities over a tropical site Gadanki (13.5\textdegree{} N, 79.2\textdegree{} E) located in south-east India. Diurnal variations of BC have two peak structures one in the morning ({$\sim$}08 IST) in all seasons and second in the evening ({$\sim$}20 IST) only during the pre-monsoon (March\textendash May). Intra-annual variation in BC indicated February and March months as the bio-mass burning with highest BC mass concentration (3000\textendash 5000 ng/m3). About 46\% of air parcel back trajectories found passing across the in-land regions of southern peninsular India brining transported aerosol to the source location during pre-monsoon. The lowest BC ({$\sim$}1500 ng/m3) is noticed during the monsoon months (June\textendash September). The average BC (2200 ng/m3) represents observational site as a typical rural site. The inter-annual variability of BC did not show any significant trend. However, trends in the maximum (March) and minimum (July) BC values show statistically significant decreasing trend suggesting reduction in bio-mass burning sources during March supported by the decrease in the fire counts. Diurnal variation in the absorption angstrom exponent indicates that the morning and evening peaks are contributed by the bio-mass combustion with values above threshold of 1. However, angstrom exponent values are found below 1 during noon time of monsoon season suggesting fossil fuel contribution. Strong coupling is found between aerosol concentration and tropospheric dynamics, meteorology in addition to the sources. The present study is expected to provide valuable input to the modelers and observational physicists as BC is climate sensitive variable.},
  langid = {english},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\BWBI4GXU\\Ravi Kiran et al. - 2018 - Long-term observations of black carbon aerosol ove.pdf}
}

@article{ravindra2016occupational,
