@incollection{velkushanova2021setting,
  title = {Setting~the~Stage},
  booktitle = {Methods for {{Faecal Sludge Analysis}}},
  author = {Strande, Linda and Velkushanova, Konstantina and Brdjanovic, Damir},
  editor = {Velkushanova, Konstantina and Strande, Linda and Ronteltap, Mariska and Koottatep, Thammarat and Brdjanovic, Damir and Buckley, Chris},
  year = {2021},
