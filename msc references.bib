@article{zotter2017evaluation,
  title = {Evaluation of the Absorption {{\AA ngstr\"om}} Exponents for Traffic and Wood Burning in the {{Aethalometer-based}} Source Apportionment Using Radiocarbon Measurements of Ambient Aerosol},
  author = {Zotter, Peter and Herich, Hanna and Gysel, Martin and {El-Haddad}, Imad and Zhang, Yanlin and Mo{\v c}nik, Gri{\v s}a and H{\"u}glin, Christoph and Baltensperger, Urs and Szidat, S{\"o}nke and Pr{\'e}v{\^o}t, Andr{\'e} S. H.},
  year = {2017},
  month = mar,
  journal = {Atmospheric Chemistry and Physics},
  volume = {17},
  number = {6},
  pages = {4229--4249},
  publisher = {{Copernicus GmbH}},
  issn = {1680-7316},
  doi = {10.5194/acp-17-4229-2017},
  urldate = {2022-08-23},
  abstract = {{$<$}p{$><$}strong class="journal-contentHeaderColor"{$>$}Abstract.{$<$}/strong{$>$} Equivalent black carbon (EBC) measured by a multi-wavelength Aethalometer can be apportioned to traffic and wood burning. The method is based on the differences in the dependence of aerosol absorption on the wavelength of light used to investigate the sample, parameterized by the source-specific absorption \AA ngstr\"om exponent (\emph{{$\alpha$}}). While the spectral dependence (defined as \emph{{$\alpha$}} values) of the traffic-related EBC light absorption is low, wood smoke particles feature enhanced light absorption in the blue and near ultraviolet. Source apportionment results using this methodology are hence strongly dependent on the \emph{{$\alpha$}} values assumed for both types of emissions: traffic \emph{{$\alpha$}}\textsubscript{TR}, and wood burning \emph{{$\alpha$}}\textsubscript{WB}. Most studies use a single \emph{{$\alpha$}}\textsubscript{TR} and \emph{{$\alpha$}}\textsubscript{WB} pair in the Aethalometer model, derived from previous work. However, an accurate determination of the source specific \emph{{$\alpha$}} values is currently lacking and in some recent publications the applicability of the Aethalometer model was questioned.{$<$}br{$><$}br{$>$}Here we present an indirect methodology for the determination of \emph{{$\alpha$}}\textsubscript{WB} and \emph{{$\alpha$}}\textsubscript{TR} by comparing the source apportionment of EBC using the Aethalometer model with \textsuperscript{14}C measurements of the EC fraction on 16 to 40 h filter samples from several locations and campaigns across Switzerland during 2005\textendash 2012, mainly in winter. The data obtained at eight stations with different source characteristics also enabled the evaluation of the performance and the uncertainties of the Aethalometer model in different environments. The best combination of \emph{{$\alpha$}}\textsubscript{TR} and \emph{{$\alpha$}}\textsubscript{WB} (0.9 and 1.68, respectively) was obtained by fitting the Aethalometer model outputs (calculated with the absorption coefficients at 470 and 950 nm) against the fossil fraction of EC (EC\textsubscript{F} / EC) derived from \textsuperscript{14}C measurements. Aethalometer and \textsuperscript{14}C source apportionment results are well correlated (\emph{r} = 0.81) and the fitting residuals exhibit only a minor positive bias of 1.6 \% and an average precision of 9.3 \%. This indicates that the Aethalometer model reproduces reasonably well the \textsuperscript{14}C results for all stations investigated in this study using our best estimate of a single \emph{{$\alpha$}}\textsubscript{WB} and \emph{{$\alpha$}}\textsubscript{TR} pair. Combining the EC, \textsuperscript{14}C, and Aethalometer measurements further allowed assessing the dependence of the mass absorption cross section (MAC) of EBC on its source. Results indicate no significant difference in MAC at 880 nm between EBC originating from traffic or wood-burning emissions. Using EC\textsubscript{F} / EC as reference and constant a priori selected \emph{{$\alpha$}}\textsubscript{TR} values, \emph{{$\alpha$}}\textsubscript{WB} was also calculated for each individual data point. No clear station-to-station or season-to-season differences in \emph{{$\alpha$}}\textsubscript{WB} were observed, but \emph{{$\alpha$}}\textsubscript{TR} and \emph{{$\alpha$}}\textsubscript{WB} values are interdependent. For example, an increase in \emph{{$\alpha$}}\textsubscript{TR} by 0.1 results in a decrease in \emph{{$\alpha$}}\textsubscript{WB} by 0.1. The fitting residuals of different \emph{{$\alpha$}}\textsubscript{TR} and \emph{{$\alpha$}}\textsubscript{WB} combinations depend on EC\textsubscript{F} / EC such that a good agreement cannot be obtained over the entire EC\textsubscript{F} / EC range using other \emph{{$\alpha$}} pairs. Additional combinations of \emph{{$\alpha$}}\textsubscript{TR} = 0.8, and 1.0 and \emph{{$\alpha$}}\textsubscript{WB} = 1.8 and 1.6, respectively, are possible but only for EC\textsubscript{F} / EC between {$\sim$} 40 and 85 \%. Applying \emph{{$\alpha$}} values previously used in the literature such as \emph{{$\alpha$}}\textsubscript{WB} of {$\sim$} 2 or any \emph{{$\alpha$}}\textsubscript{WB} in combination with \emph{{$\alpha$}}\textsubscript{TR} = 1.1 to our data set results in large residuals. Therefore we recommend to use the best \emph{{$\alpha$}} combination as obtained here (\emph{{$\alpha$}}\textsubscript{TR} = 0.9 and \emph{{$\alpha$}}\textsubscript{WB} = 1.68) in future studies when no or only limited additional information like \textsuperscript{14}C measurements are available. However, these results were obtained for locations impacted by black carbon (BC) mainly from traffic consisting of a modern car fleet and residential wood combustion with well-constrained combustion efficiencies. For regions of the world with different combustion conditions, additional BC sources, or fuels used, further investigations are needed.{$<$}/p{$>$}},
  langid = {english},
  file = {C\:\\Users\\Mabel Karani\\Zotero\\storage\\FCBB6YPZ\\Zotter et al. - 2017 - Evaluation of the absorption Ã…ngstrÃ¶m exponents fo.pdf;C\:\\Users\\Mabel Karani\\Zotero\\storage\\U4PV5MAQ\\2017.html}
}
