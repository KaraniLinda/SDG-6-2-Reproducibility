  shorttitle = {Theoretical {{Framework}} for {{Plastic Waste Management}} in {{Ghana}} through {{Extended Producer Responsibility}}},
  author = {Quartey, Ebo Tawiah and Tosefa, Hero and Danquah, Kwasi Asare Baffour and Obrsalova, Ilona},
  year = {2015},
